<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="@drawable/background"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/credit_view"
        android:layout_width="404dp"
        android:layout_height="108dp"
        android:layout_margin="16dp"
        android:fontFamily="@font/stenc_ex"
        android:text="Credits"
        android:textAlignment="center"
        android:textColor="@color/dBlue"
        android:textSize="100dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.054" />


    <ImageView
        android:id="@+id/textView2"
        android:layout_width="368dp"
        android:layout_height="431dp"
        android:contentDescription="background"
        android:scaleType="fitXY"
        android:src="@drawable/purewhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.488"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/credit_view"
        app:layout_constraintVertical_bias="0.09"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintTop_creator="1" />

    <Button
        android:id="@+id/bt_back"
        android:layout_width="122dp"
        android:layout_height="110dp"
        android:layout_marginTop="4dp"
        android:backgroundTint="@color/white"
        android:contentDescription="credits"
        android:fontFamily="@font/stenc_ex"
        android:text="Back"
        android:textColor="@color/dBlue"
        android:textSize="30dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2"
        app:layout_constraintVertical_bias="0.959"
        tools:layout_constraintBottom_creator="1"
        tools:layout_constraintLeft_creator="1"
        tools:layout_constraintRight_creator="1" />

    <LinearLayout
        android:layout_width="352dp"
        android:layout_height="418dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="@+id/textView2"
        app:layout_constraintEnd_toEndOf="@+id/textView2"
        app:layout_constraintHorizontal_bias="0.567"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/credit_view"
        app:layout_constraintVertical_bias="1.0">

        <TextView
            android:id="@+id/credit_text1"
            android:layout_width="332dp"
            android:layout_height="48dp"
            android:layout_margin="16dp"
            android:fontFamily="@font/stenc_ex"
            android:text="Brandon Fischer"
            android:textAlignment="center"
            android:textColor="@color/dBlue"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.392"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.228" />

        <TextView
            android:id="@+id/credit_text2"
            android:layout_width="332dp"
            android:layout_height="48dp"
            android:layout_margin="16dp"
            android:fontFamily="@font/stenc_ex"
            android:text="Ricardo Reyes"
            android:textAlignment="center"
            android:textColor="@color/dBlue"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.341" />

        <TextView
            android:id="@+id/credit_text3"
            android:layout_width="332dp"
            android:layout_height="48dp"
            android:layout_margin="16dp"
            android:fontFamily="@font/stenc_ex"
            android:text="Raymond Harmon"
            android:textAlignment="center"
            android:textColor="@color/dBlue"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.563" />

        <TextView
            android:id="@+id/credit_text4"
            android:layout_width="332dp"
            android:layout_height="48dp"
            android:layout_margin="16dp"
            android:fontFamily="@font/stenc_ex"
            android:text="Jason Engelbrecht"
            android:textAlignment="center"
            android:textColor="@color/dBlue"
            android:textSize="40dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.493"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.45" />


    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>